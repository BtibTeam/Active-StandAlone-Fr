<?xml version="1.0" encoding="UTF-8"?>
<!-- Niagara Presentation XML -->
<px version="1.0">
<import>
  <module name="baja"/>
  <module name="bajaui"/>
  <module name="btibCore"/>
  <module name="btibStructure"/>
  <module name="btibVision"/>
  <module name="converters"/>
  <module name="gx"/>
  <module name="kitPx"/>
</import>
<properties>

  <property name="nomSite" type="baja:String" value="station:|slot:/Accueil/Param$e8tres$20stations/Param$e8tres$20g$e9n$e9raux/nomSite">
  </property>

  <property name="ordSite" type="baja:Ord" value="station:|slot:/Accueil/Navigation$20G$e9ographique">
  </property>

  <property name="gaucheForegroundCouleur" type="gx:Brush" value="white">
  </property>

  <property name="gaucheBackgroundCouleurClaire" type="gx:Brush" value="#616161">
  </property>

  <property name="gaucheBackgroundCouleur" type="gx:Brush" value="#424242">
  </property>

  <property name="gaucheBackgroundCouleurSombre" type="gx:Brush" value="#212121">
  </property>

  <property name="enTeteForegroundCouleur" type="gx:Brush" value="white">
  </property>

  <property name="enTeteBackgroundCouleurClaire" type="gx:Brush" value="#039be5">
    <target ord="slot:content/OverBorderPane/content/CanvasPane/background"/>
  </property>

  <property name="enTeteBackgroundCouleur" type="gx:Brush" value="#0288d1">
  </property>

  <property name="enTeteBackgroundCouleurSombre" type="gx:Brush" value="#0277bd">
  </property>

  <property name="bqlToWidgetsHandle" type="baja:String" value="4811">
  </property>

  <property name="vueCentraleForegroundNoire" type="gx:Brush" value="black">
  </property>

  <property name="vueCentraleBackgroundBlanc" type="gx:Brush" value="white">
  </property>

  <property name="vueCentraleForegroundCouleur" type="gx:Brush" value="white">
  </property>

  <property name="vueCentraleBackgroundCouleurClaire" type="gx:Brush" value="#546e7a">
  </property>

  <property name="vueCentraleBackgroundCouleur" type="gx:Brush" value="#455a64">
  </property>

  <property name="vueCentraleBackgroundCouleurSombre" type="gx:Brush" value="#37474f">
  </property>

  <property name="vueCentraleElementForegroundCouleur" type="gx:Brush" value="white">
  </property>

  <property name="vueCentraleElementBackgroundCouleurClaire" type="gx:Brush" value="#1e88e5">
  </property>

  <property name="vueCentraleElementBackgroundCouleur" type="gx:Brush" value="#1976d2">
  </property>

  <property name="vueCentraleElementBackgroundCouleurSombre" type="gx:Brush" value="#1565c0">
  </property>

  <property name="logoWhite" type="gx:Image" value="file:^images/logos/logo_w.svg">
  </property>

</properties>
<content>
<ScrollPane>

  <CanvasPane name="content" viewSize="1000.0,800.0" scale="fitRatio" minScaleFactor="0.5" maxScaleFactor="1.0">

    <Polygon fill="#b32326b8" stroke="white" pen="0.0" geom="410.0,170.0 390.0,240.0 600.0,230.0 790.0,230.0 800.0,160.0 590.0,130.0 ">
      <ValueBinding hyperlink="$(ordEtage)"/>
      <String name="id" value="widget83a7"/>
      <IdBinding id="etageDansBatiment"/>
    </Polygon>

    <Label layout="200.0,310.0,80.0,60.0" image="module://themeBtib/image/x30/devices/electricity/powerMeasurementDevice_ok.png" font="14.0pt Calibri" textToIconAlign="bottom" textIconGap="0.0">
      <IdBinding id="Compteur"/>
      <SourceBinding ord="station:|$(slotPathOrd)"/>
      <ValueBinding ord="station:|$(slotPathOrd)">
        <ObjectToString name="text" format="%displayName%"/>
      </ValueBinding>
    </Label>

    <Label layout="300.0,310.0,80.0,60.0" image="module://themeBtib/image/x30/devices/hvac/ahu_ok.png" font="14.0pt Calibri" textToIconAlign="bottom" textIconGap="0.0">
      <ValueBinding ord="station:|$(slotPathOrd)" hyperlink="station:|$(slotPathOrd)">
        <ObjectToString name="text" format="%displayName%"/>
      </ValueBinding>
      <IdBinding id="CTA"/>
    </Label>

    <Label layout="400.0,310.0,80.0,60.0" image="file:^images/icones/capteurLumiereOcc.png" font="14.0pt Calibri" textToIconAlign="bottom" textIconGap="0.0">
      <IdBinding id="Multicapteur"/>
      <SourceBinding ord="station:|$(slotPathOrd)"/>
    </Label>

    <GridPane layout="100.0,310.0,80.0,60.0" columnCount="1" columnAlign="center" rowGap="4.0">

      <Label font="10.0pt Calibri" textToIconAlign="bottom" textIconGap="0.0">
        <ValueBinding ord="station:|$(slotPathOrd)/points/Space$20Temp">
          <ObjectToString name="text" format="%out.value%"/>
        </ValueBinding>
      </Label>

      <Label image="module://themeBtib/image/x30/devices/hvac/fanCoilUnit_ok.png" font="14.0pt Calibri Light" textToIconAlign="bottom" textIconGap="0.0">
        <PopupBinding ord="station:|$(slotPathOrd)|view:btibVision:FlexPane" title="D&#xe9;tails" size="1200.0,800.0"/>
        <ValueBinding ord="station:|$(slotPathOrd)/master"/>
      </Label>

      <SourceBinding ord="station:|$(slotPathOrd)"/>

      <IdBinding id="VC"/>

    </GridPane>

    <Label layout="500.0,310.0,80.0,60.0" image="file:^images/icones/controleurMaitre.png" font="14.0pt Calibri" textToIconAlign="bottom" textIconGap="0.0">
      <IdBinding id="Contr&#xf4;leur d&#x27;&#xe9;clairage"/>
      <SourceBinding ord="station:|$(slotPathOrd)"/>
    </Label>

    <Label layout="600.0,310.0,80.0,60.0" image="module://themeBtib/image/x30/metiers/lighting_down.png" font="14.0pt Calibri" textToIconAlign="bottom" textIconGap="0.0">
      <IdBinding id="Lampe"/>
      <SourceBinding ord="station:|$(slotPathOrd)"/>
    </Label>

    <OverBorderPane layout="100.0,40.0,220.0,210.0" padding="0.0" fill="white" radius="10.0px 10.0px 10.0px 10.0px">

      <GridPane name="content" columnCount="1" halign="fill" rowAlign="fill" columnAlign="fill" rowGap="0.0" columnGap="0.0" stretchRow="1" stretchColumn="0" bandBrush="white">

        <CanvasPane viewSize="220.0,100.0" background="#039be5">

          <Label layout="10.0,10.0,200.0,50.0" text="$(nomBatiment)" font="20.0pt Calibri" foreground="white" halign="left" wordWrapEnabled="true">
            <ValueBinding hyperlink="$(ordBatiment)"/>
          </Label>

          <Label layout="10.0,60.0,200.0,35.0" text="$(descriptionBatiment)" font="14.0pt Calibri" foreground="white" halign="left" valign="top" wordWrapEnabled="true">
            <ValueBinding hyperlink="$(ordBatiment)"/>
          </Label>

        </CanvasPane>

        <CanvasPane viewSize="220.0,110.0" background="white">

          <GridPane layout="10.0,0.0,200.0,100.0" columnCount="1" halign="left" columnAlign="fill" rowGap="10.0" stretchColumn="0">

            <Label image="module://themeBtib/image/x16/status/alarm.png" font="14.0pt Calibri Light" foreground="#454545" halign="left" wordWrapEnabled="true">
              <ValueBinding ord="$(synthesePointsAlarmesOrd)/value">
                <ObjectToString name="text" format="%.% points en alarme"/>
              </ValueBinding>
            </Label>

            <Label image="module://themeBtib/image/x16/status/overridden.png" font="14.0pt Calibri Light" foreground="#454545" halign="left" wordWrapEnabled="true">
              <ValueBinding ord="$(synthesePointsDerogesOrd)/value">
                <ObjectToString name="text" format="%.% points d&#xe9;rog&#xe9;s"/>
              </ValueBinding>
            </Label>

            <Label image="module://themeBtib/image/x16/status/fault.png" font="14.0pt Calibri Light" foreground="#454545" halign="left" wordWrapEnabled="true">
              <ValueBinding ord="$(synthesePointsEnDefautOrd)/value">
                <ObjectToString name="text" format="%.% points en d&#xe9;faut"/>
              </ValueBinding>
            </Label>

          </GridPane>

        </CanvasPane>

      </GridPane>

      <IdBinding id="batimentDansSite"/>

    </OverBorderPane>

  </CanvasPane>

</ScrollPane>
</content>
</px>
